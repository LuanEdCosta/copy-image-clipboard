{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5", "dom"],
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "strict": true,
    "declaration": true,
    "removeComments": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "emitDeclarationOnly": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["**/*.test.ts"],
  "include": ["src"]
}
